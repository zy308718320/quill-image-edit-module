{
  "name": "quill-image-edit-module",
  "version": "0.1.0",
  "description": "A module for Quill rich text editor that allows editing of images.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest --coverage --no-cache",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "webpack && tsc",
    "build:dev": "webpack -w && tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint --fix --ext .ts src/",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zy308718320/quill-image-edit-module.git"
  },
  "author": "evanzhou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zy308718320/quill-image-edit-module/issues"
  },
  "homepage": "https://github.com/zy308718320/quill-image-edit-module#readme",
  "dependencies": {
    "lodash": "^4.17.21",
    "parchment": "^2.0.0-dev.2",
    "quill": "^1.3.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/quill": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^7.31.0",
    "jest": "^27.0.6",
    "raw-loader": "^4.0.2",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "webpack": "^5.47.1",
    "webpack-cli": "^4.7.2"
  },
  "standard-version": {
    "scripts": {
      "posttag": "git push --follow-tags origin master"
    }
  }
}
